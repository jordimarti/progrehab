<%= simple_form_for(@ingres, remote: true) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    
    <div class="row">
      <div class="medium-6 columns end">
        <p><%= t('.data') %>: <%= @ingres.data_mes %>/<%= @ingres.data_any %></p>
        <%= f.input :import %>
      </div>
    </div>

  </div>

  <div class="form-actions">
    <%= f.button :submit, t('.guardar'), :class => 'button button-right' %>
  </div>
<% end %>
